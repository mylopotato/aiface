{
    "bridge": "AifaceBridge",
    "host": "127.0.0.1",
    "port": 8080,
    "workers": 8,
    "app-env": "dev",
    "debug": 0,
    "logging": 1,
    "static-directory": "",
    "bootstrap": false,
    "max-requests": 1000,
    "max-execution-time": 30,
    "memory-limit": -1,
    "ttl": 0,
    "populate-server-var": true,
    "socket-path": ".ppm\/run\/",
    "pidfile": ".ppm\/ppm.pid",
    "reload-timeout": 30,
    "cgi-path": "\/usr\/bin\/php"
}